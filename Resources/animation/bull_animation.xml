<object_animation name="bull" plist_name="pig_spritesheet.plist" image_name="pig_spritesheet.png">
    <animation name="run" delay="0.06" loop="infinity">
        <origin x="0.55" y="0.5" />
        <framelist>
            <frame name="bull_run_1.png" />
            <frame name="bull_run_2.png" />
            <frame name="bull_run_3.png" />
            <frame name="bull_run_4.png" />
            <frame name="bull_run_5.png" />
            <frame name="bull_run_6.png" />
            <frame name="bull_run_7.png" />
            <frame name="bull_run_8.png" />
            <frame name="bull_run_9.png" />
            <frame name="bull_run_10.png" />
        </framelist>
    </animation>
    <animation name="jump" delay="0.05" loop="1">
      <origin x="0.65" y="0.4" />
        <framelist>
            <frame name="bull_run_1.png" />
            <frame name="bull_run_2.png" />
            <frame name="bull_run_3.png" />
        </framelist>
    </animation>
    
    <animation name="attack" delay="0.1" loop="1">
        <origin x="0.55" y="0.5" />
        <framelist>
            <frame name="bull_run_1.png" />
             <frame name="bull_run_2.png" />
        </framelist>
    </animation>
    
    
    <animation name="fall" delay="0.1" loop="infinity">
        <origin x="0.6" y="0.4" />
        <framelist>
            <frame name="bull_run_1.png" />
            <frame name="bull_run_2.png" />
            <frame name="bull_run_3.png" />
            <frame name="bull_run_4.png" />
            <frame name="bull_run_5.png" />
            <frame name="bull_run_6.png" />
            <frame name="bull_run_7.png" />
            <frame name="bull_run_8.png" />
            <frame name="bull_run_9.png" />
            <frame name="bull_run_10.png" />
        </framelist>
    </animation>

    <animation name="fly" delay="0.1" loop="infinity">
        <origin x="0.65" y="0.4" />
        <framelist>
            <frame name="bull_run_1.png" />
            <frame name="bull_run_2.png" />
            <frame name="bull_run_3.png" />
            <frame name="bull_run_4.png" />
            <frame name="bull_run_5.png" />
            <frame name="bull_run_6.png" />
            <frame name="bull_run_7.png" />
            <frame name="bull_run_8.png" />
            <frame name="bull_run_9.png" />
            <frame name="bull_run_10.png" />
        </framelist>
    </animation>

    <animation name="skill" delay="0.1" loop="1">
        <origin x="0.3" y="0.22" />
        <framelist>
            <frame name="bull_run_1.png" />
            <frame name="bull_run_2.png" />
         </framelist>

    </animation>

    <animation name="idle" delay="0.1" loop="infinity">
        <origin x="0.54" y="0.5" />
        <framelist>
            <frame name="bull_run_1.png" />
            <frame name="bull_run_2.png" />
            <frame name="bull_run_3.png" />
            <frame name="bull_run_4.png" />
            <frame name="bull_run_5.png" />
            <frame name="bull_run_6.png" />
            <frame name="bull_run_7.png" />
            <frame name="bull_run_8.png" />
            <frame name="bull_run_9.png" />
            <frame name="bull_run_10.png" />        </framelist>
    </animation>
</object_animation>